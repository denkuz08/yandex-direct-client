{
  "name": "gladyshev/yandex-direct-client",
  "keywords": ["yandex", "direct", "api", "client", "lib", "library"],
  "description": "Yandex Direct API PHP-client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru",
      "homepage": "https://github.com/gladyshev"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4",
    "psr/log": "1.0.2",
    "guzzlehttp/guzzle": "5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
